#include <stdio.h>
#include <locale.h>

void main(void)
{
	setlocale(LC_ALL, "RUS");
	int num, lol;
	puts("введите число", "\n");
	scanf("%d", &num);
	puts("введено число A");
	puts("введите число B");
	scanf("%d", &lol);
	puts("введено число B");
	printf("A+B=%d\n", lol + num);
	printf("A-B=%d\n", lol - num);
	printf("A*B=%d\n", lol * num);
	printf("частное В/A=%d\n", lol / num);
	printf("остаток от В/А=%d\n", lol % num);
}
